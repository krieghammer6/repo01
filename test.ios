version 15.6
service timestamps debug datetime msec
service timestamps log datetime msec
no service password-encryption
!
hostname R1
!
boot-start-marker
boot-end-marker
!
!
enable password cisco
!
aaa new-model
!
!
aaa group server radius ISE-Radius
 server name ISE
!
aaa authentication login default group ISE-Radius
aaa authentication d!
!
!
!
!
aaa session-id common
ethernet lmi ce
!
!
!
clock timezone EST -5 0
clock summer-time EDT recurring
mmi polling-interval 60
no mmi auto-configure
no mmi pvc
mmi snmp-timeout 180
!
!
!
!
!
!
!
!
!
!
!
no ip domain lookup
ip cef
ipv6 unicast-routing
ipv6 cef
!
multilink bundle-name authenticated
!
!
!
!
!
redundancy
!
!
! 
!
!
!
!
!
!
!
!
!
!
!
!
interface Loopback0
 description Loopback
 ip address 192.168.0.1 255.255.255.255
!
interface GigabitEthernet0/0
 description OOB Management
 ip address 172.16.1.55 255.255.255.0
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/1
 description to Switch
 ip address 22.0.0.2 255.255.255.0
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/2
 description to ISP1
 ip address 206.206.206.2 255.255.255.0
 duplex auto
 speed auto
 media-type rj45
!
interface GigabitEthernet0/3
 description to ISP2
 ip address 12.12.12.2 255.255.255.0
 duplex auto
 speed auto
 media-type rj45
!
ip forward-protocol nd
!
!
no ip http server
no ip http secure-server
ip route 10.1.100.0 255.255.255.0 172.16.1.1
ip route 10.10.96.0 255.255.252.0 172.16.1.1
!
logging source-interface GigabitEthernet0/0
logging host 10.10.98.40
!
snmp-server community udtlab RO
snmp-server community udtnet RW
snmp-server location Tally
snmp-server contact mhanna@udtonline.com
snmp-server chassis-id 
!
!
!
radius server ISE
 address ipv4 10.1.100.21 auth-port 1645 acct-port 1646
 key supersecret
!
!
control-plane
!
